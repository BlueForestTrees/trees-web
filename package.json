    "d3": "^5.1.0",
    "request-promise-lite": "^0.12.0",
    "trees-units": "^1.0.1",
    "vue": "^2.5.16",
    "vuetify": "^1.0.16",
    "vuex": "^3.0.1",
    "window-or-global": "^1.0.1"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "babel": {
    "presets": [
      "env",
      "stage-3"
