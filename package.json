{
  "name": "web-blueforest",
  "version": "1.8.9",
  "description": "web for trees",
  "main": "index.js",
  "author": "slim",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-serve --port 8079 --config webpack.config.js",
    "dev-on-integ": "cross-env NODE_ENV=development TARGET=integ webpack-serve --port 8079 --config webpack.config.js",
    "dev-on-prod": "cross-env NODE_ENV=development TARGET=prod webpack-serve --port 8079 --config webpack.config.js",
    "test": "cross-env NODE_ENV=development mocha --recursive --require @babel/register --exit \"test/test*.js\"",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack",
    "build-stats": "rimraf dist && cross-env NODE_ENV=production STATS=true node --trace-deprecation node_modules/webpack/bin/webpack",
    "deploy": "yarn run build && scp -r dist/www/. pi@one.blueforest.org:/var/www"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-transform-runtime": "7.0.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/register": "7.0.0",
    "@babel/runtime": "7.0.0",
    "@vue/test-utils": "1.0.0-beta.20",
    "babel-loader": "8.0.4",
    "chai": "4.1.2",
    "connect-history-api-fallback": "1.5.0",
    "copy-webpack-plugin": "4.5.2",
    "cross-env": "5.1.4",
    "css-loader": "1.0.0",
    "html-webpack-plugin": "3.0.6",
    "http-proxy-middleware": "0.18.0",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "koa-connect": "2.0.1",
    "lodash-webpack-plugin": "0.11.5",
    "mocha": "5.2.0",
    "mongodb": "2.2.36",
    "node-notifier": "5.1.2",
    "node-sass": "4.8.3",
    "nyc": "11.3.0",
    "rimraf": "2.6.2",
    "style-loader": "0.23.0",
    "uglifyjs-webpack-plugin": "1.2.4",
    "vue-loader": "15.0.7",
    "vue-style-loader": "3.0.3",
    "vue-template-compiler": "2.5.17",
    "webpack": "4.23.1",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-command": "0.4.1",
    "webpack-node-externals": "1.7.2",
    "webpack-serve": "2.0.2",
    "webpack-visualizer-plugin": "0.1.11"
  },
  "dependencies": {
    "@material/button": "0.26.0",
    "jwt-decode": "2.2.0",
    "lodash.debounce": "4.0.8",
    "unit-manip": "^1.0.7",
    "vis": "4.21.0",
    "vue": "2.5.17",
    "vue-infinite-loading": "2.3.1",
    "vue-resource": "1.5.1",
    "vue-router": "3.0.1",
    "vuedraggable": "2.17.0",
    "vuetify": "1.3.7",
    "vuex": "3.0.1",
    "window-or-global": "1.0.1"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-syntax-dynamic-import"
    ],
    "env": {
      "test": {
        "plugins": []
      }
    }
  }
}
